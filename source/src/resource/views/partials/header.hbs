<style>
    .navbar {
        padding: 0px !important;
    }

    .menu {
        font-size: 18px !important;
    }

    .nav-link:hover {
        color: aqua !important;
    }

    .dropdown:hover .dropdown-menu {
        display: block;
    }

    .dropdown-item {
        background-color: black;
        color: blue;
    }

    .dropdown-menu .menu:hover {
        color: aqua !important;
        background-color: black !important
    }

    #name__user {
        color: #a0a3a7;
        font-weight: 400;
    }

    #name__user:hover {
        color: orangered;
        cursor: pointer;
    }

    .user {
        position: relative;
    }

    .dropdown__hover {
        display: none;
        position: absolute;
        left: 34px;
        min-width: 150px;
        background-color: #fff;
        border: 1px solid #ccc;
        border-radius: 2px;
    }

    .dropdown__hover span,
    a {
        padding: 4px 8px;
        font-weight: 1rem;
    }

    .dropdown__hover a {
        text-decoration: none;
        color: #000;
    }

    .dropdown__hover span:hover {
        background-color: #ccc;
        cursor: pointer;
    }

    .dropdown__hover a:hover {
        background-color: #ccc;
        cursor: pointer;
        text-decoration: none;
        color: #000;
    }

    .user:hover .dropdown__hover {
        display: flex;
    }
</style>

<!-- Header -->
<nav class="navbar bg-body-tertiary">
    <div class="container">
        <a class="navbar-brand" href="#">
            <img id="img__logo"
                src="data:image/png;base64,"
                alt="Logo" height="80">
        </a>
        <form class="d-flex" role="search" method="GET" action="/tim-kiem">
            <input class="form-control me-1" type="search" placeholder="Tìm tên phim" aria-label="Search"
                name="keyword">
            <button class="btn btn-outline-success" type="submit" id="search-btn">
                <i class="fa fa-search"></i>
            </button>
        </form>

        <!-- Flash message -->
        {{#if nameUser}}
        <div class="user">
            <img id="icon__user" src="/img/user.png" alt="icon__user">
            <span id="name__user">{{ nameUser }}</span>

            <div class="dropdown__hover  flex-column">
                <a href="/thanh-vien" class="w-100">Tài khoản</a>
                <span id="logout" class="w-100 ">Đăng xuất</span>
            </div>
        </div>
        {{else}}
        <a href="/login" class="navbar-brand"><i class='fa fa-user'></i> Đăng nhập</a>
        {{/if}}
        <!-- End Flash message -->

        {{!-- <a href="/login" class="navbar-brand"><i class='fa fa-user'></i> Đăng nhập</a> --}}
    </div>

    <div class="navbar-expand-lg container-fluid bg-black">
        <button class="navbar-toggler" type="button" data-bs-toggle="collapse" data-bs-target="#navbarNav"
            aria-controls="navbarNav" aria-expanded="false" aria-label="Toggle navigation">
            <span class="navbar-toggler-icon bg-white"></span>
        </button>
        <div class="collapse navbar-collapse" id="navbarNav" style="justify-content: center;">
            <ul style="width: 80%; display: flex;" class="navbar-nav justify-content-around">
                <li class="nav-item menu">
                    <a class="nav-link active text-white" aria-current="page" href="/mua-ve">MUA VÉ</a>
                </li>
                <li class="nav-item dropdown menu">
                    <a class="nav-link dropdown-toggle text-white" href="#" role="button" data-bs-toggle=""
                        aria-expanded="false">
                        PHIM
                    </a>
                    <ul class="dropdown-menu" style="background-color: black;">
                        <li><a class="dropdown-item menu" style="color: white;" href="/phim-dang-chieu">PHIM ĐANG
                                CHIẾU</a></li>
                        <li><a class="dropdown-item menu" style="color: white;" href="/phim-sap-chieu">PHIM SẮP
                                CHIẾU</a></li>
                    </ul>
                </li>
                <li class="nav-item dropdown menu">
                    <a class="nav-link dropdown-toggle text-white" href="#" role="button" data-bs-toggle=""
                        aria-expanded="false">
                        GÓC ĐIỆN ẢNH
                    </a>
                    <ul class="dropdown-menu" style="background-color: black;">
                        <li><a class="dropdown-item menu" style="color: white;" href="/top-phim">TOP PHIM HAY</a></li>
                        <li><a class="dropdown-item menu" style="color: white;" href="/dien-vien">DIỄN VIÊN</a></li>
                        <li><a class="dropdown-item menu" style="color: white;" href="/dao-dien">ĐẠO DIỄN</a></li>
                        <li><a class="dropdown-item menu" style="color: white;" href="/binh-luan-phim">BÌNH LUẬN
                                PHIM</a></li>
                        <li><a class="dropdown-item menu" style="color: white;" href="/blog-dien-anh">BLOG ĐIỆN ẢNH</a>
                        </li>
                    </ul>
                </li>
                {{!-- <li class="nav-item dropdown menu">
                    <a class="nav-link dropdown-toggle text-white" role="button" data-bs-toggle=""
                        aria-expanded="false">
                        SỰ KIỆN
                    </a>
                    <ul class="dropdown-menu" style="background-color: black;">
                        <li><a class="dropdown-item menu" style="color: white;" href="/uu-dai">ƯU ĐÃI</a></li>
                    </ul>
                </li> --}}
                <li class="nav-item menu">
                    <a class="nav-link active text-white" aria-current="page" href="/uu-dai">ƯU ĐÃI</a>
                </li>
                <li class="nav-item menu">
                    <a class="nav-link active text-white" aria-current="page" href="/ho-tro">HỖ TRỢ</a>
                </li>
                <li class="nav-item menu">
                    <a class="nav-link active text-white" aria-current="page" href="/chinh-sach">THÀNH VIÊN</a>
                </li>
            </ul>
        </div>
    </div>
</nav>
<!-- End Header -->

<script>
    img__logo = document.getElementById("img__logo");
    img__logo.addEventListener("click", function (e) {
        window.location.href = '/'
    })

    {{#if nameUser}}
        const logout = document.getElementById("logout")
        logout.addEventListener("click", function (e) {
        fetch("/logout", {
            method: "POST",
        })
            .then((response) => {
                window.location.href = '/'
            })
            .catch((error) => {
                console.log("Lỗi khi gửi yêu cầu đăng xuất", error);
            });
        });
    {{/if}}
</script>